{
    "name": "abbotton/meituan-takeaway",
    "description": "美团外卖开放平台PHP SDK",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Abbotton",
            "email": "uctoo@foxmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Abbotton\\MeituanTakeaway\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abbotton\\MeituanTakeaway\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
